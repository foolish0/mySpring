<?xml version="1.0" encoding="UTF-8" ?>
<beans>
    <bean id="aservice" class="com.myspring.test.AServiceImpl">
        <constructor-arg type="String" name="name" value="abc"/>
        <constructor-arg type="int" name="level" value="3"/>
        <property type="String" name="property1" value="Someone says"/>
        <property type="String" name="property2" value="Hello World!"/>
        <property type="com.myspring.test.BaseService" name="baseService" ref="baseservice"/>
    </bean>

    <bean id="baseservice" class="com.myspring.test.BaseService">
<!--        <property type="com.myspring.test.BaseBaseService" name="baseBaseService" ref="basebaseservice"/>-->
    </bean>

    <bean id="basebaseservice" class="com.myspring.test.BaseBaseService">
        <property type="com.myspring.test.AServiceImpl" name="as" ref="aservice"/>
    </bean>
</beans>
